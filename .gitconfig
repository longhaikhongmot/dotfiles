[user]
	name = Long
	email = 78641196+longwindingroad@users.noreply.github.com
	username = longwindingroad
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
[alias]
	ll = "log --color --graph --pretty=\"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset\" --abbrev-commit"
	adog = "log --all --decorate --oneline --graph"
	b = "branch -vv"
	st = "status --short"
	bc = "! git branch | grep -vE \"master|main\" | xargs git branch -D"
